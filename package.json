    "@babel/preset-typescript": "^7.3.3",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.13.8",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "babel-loader": "^8.0.5",
    "eslint-config-prettier": "^4.2.0",
    "fork-ts-checker-webpack-plugin": "^1.2.0",
    "jest": "^24.7.1",
